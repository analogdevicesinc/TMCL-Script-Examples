SGP 0, 2, 0    //set user variable #0
//for Loop
forLoop:
	// add your code here //
	GGP 0, 2	   //get user variable #0 to accu
	CALC ADD, 1    //add 1 to accu
	AGP 0, 2       //accu to user variable #0
	COMP 5		   //amount of loops
	JC LE, forLoop //jump to label forLoop

STOP
	